<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- The following two rules blame many places where passing/taking a modifiable object is OK (e.g., constructor injection). -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/> <!-- Passing Arrays as argument in Record constructor-->
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/> <!-- Constructor Autowiring -->
    </Match>
</FindBugsFilter>
